{
  "name": "vui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target=lib src/umd.js --name vui --dest dist --report --report-json --formats umd-min && python deploy.py",
    "build-demo": "vue-cli-service build --dest=demo",
    "lint": "vue-cli-service lint",
    "deploy": "python deploy.py"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "https://github.com/cristianizzo/ckeditor5-build-classic",
    "@johmun/vue-tags-input": "^2.0.1",
    "vee-validate": "^2.2.10",
    "vue-color": "^2.7.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.5.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "vue": "^2.6.10",
    "vue-slider-component": "^2.8.12",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
